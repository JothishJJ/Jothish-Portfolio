{
  "name": "@sanity/runtime-cli",
  "description": "Sanity's Runtime CLI for Blueprints and Functions",
  "version": "10.11.1",
  "author": "Sanity Runtime Team",
  "type": "module",
  "license": "MIT",
  "repository": "sanity-io/runtime-cli",
  "bugs": "https://github.com/sanity-io/runtime-cli/issues",
  "homepage": "https://github.com/sanity-io/runtime-cli",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./actions/blueprints": {
      "types": "./dist/actions/blueprints/index.d.ts",
      "import": "./dist/actions/blueprints/index.js"
    },
    "./actions/functions": {
      "types": "./dist/actions/functions/index.d.ts",
      "import": "./dist/actions/functions/index.js"
    },
    "./actions/sanity": {
      "types": "./dist/actions/sanity/index.d.ts",
      "import": "./dist/actions/sanity/index.js"
    },
    "./cores": {
      "types": "./dist/cores/index.d.ts",
      "import": "./dist/cores/index.js"
    },
    "./cores/blueprints": {
      "types": "./dist/cores/blueprints/index.d.ts",
      "import": "./dist/cores/blueprints/index.js"
    },
    "./cores/functions": {
      "types": "./dist/cores/functions/index.d.ts",
      "import": "./dist/cores/functions/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "engines": {
    "node": ">=20.19"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "bin": {
    "sanity-run": "./bin/run.js"
  },
  "scripts": {
    "build": "rollup -c && npm run build:ts && npm run build:static",
    "postbuild": "oclif manifest && oclif readme",
    "build:ts": "shx rm -rf dist *.tsbuildinfo && tsc -b",
    "build:static": "npm run copy:wrapper && npm run copy:server",
    "clean": "shx rm -rf dist oclif.manifest.json",
    "copy:server": "shx cp -r ./src/server/static ./dist/server",
    "copy:wrapper": "shx cp ./src/utils/child-process-wrapper.js ./dist/utils/child-process-wrapper.js",
    "lint": "biome ci",
    "lint:write": "biome check --write",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "pretest": "cd test/integration && npm install",
    "test": "vitest run",
    "test:depmgmt": "vitest run --config ./test-depmgmt/vitest.config.ts",
    "test:integration": "cd test/integration && npm run typecheck",
    "typecheck": "tsc --project tsconfig.typecheck.json",
    "posttest": "npm run lint",
    "test:watch": "vitest",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@architect/hydrate": "^4.0.10",
    "@architect/inventory": "^4.0.9",
    "@oclif/core": "^4.5.4",
    "@oclif/plugin-help": "^6.2.33",
    "@sanity/blueprints-parser": "^0.2.1",
    "@sanity/client": "^7.11.2",
    "adm-zip": "^0.5.16",
    "array-treeify": "^0.1.5",
    "cardinal": "^2.1.1",
    "chalk": "^5.6.2",
    "eventsource": "^4.0.0",
    "find-up": "^8.0.0",
    "get-folder-size": "^5.0.0",
    "groq-js": "^1.18.0",
    "inquirer": "^12.9.6",
    "jiti": "^2.6.0",
    "mime-types": "^3.0.1",
    "ora": "^9.0.0",
    "tar-stream": "^3.1.7",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "ws": "^8.18.3",
    "xdg-basedir": "^5.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/state": "^6.5.2",
    "@enhance/store": "^1.0.2",
    "@lezer/highlight": "^1.2.1",
    "@oclif/test": "^4.1.14",
    "@playwright/test": "^1.55.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@sanity/blueprints": "^0.3.0",
    "@sanity/functions": "^1.0.3",
    "@types/adm-zip": "^0.5.7",
    "@types/cardinal": "^2.1.1",
    "@types/mime-types": "^3.0.1",
    "@types/node": "20",
    "@types/tar-stream": "^3.1.4",
    "@types/ws": "^8.18.1",
    "codemirror": "^6.0.2",
    "mentoss": "^0.11.0",
    "oclif": "^4.22.24",
    "pretty-bytes": "^7.1.0",
    "pretty-ms": "^9.3.0",
    "rollup": "^4.52.0",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "3.2.4"
  },
  "oclif": {
    "bin": "sanity-run",
    "dirname": "sanity-run",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "state": "beta",
    "topicSeparator": " ",
    "topics": {
      "blueprints": {
        "description": "Blueprint deployment and management commands"
      },
      "functions": {
        "description": "Sanity Function development and management commands"
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": false
  }
}
