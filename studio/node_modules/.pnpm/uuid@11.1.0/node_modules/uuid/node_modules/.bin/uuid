#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules/uuid/dist/esm/bin/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules/uuid/dist/esm/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules/uuid/dist/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules/uuid/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules/uuid/dist/esm/bin/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules/uuid/dist/esm/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules/uuid/dist/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules/uuid/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/uuid@11.1.0/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/esm/bin/uuid" "$@"
else
  exec node  "$basedir/../../dist/esm/bin/uuid" "$@"
fi
