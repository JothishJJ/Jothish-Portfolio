#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/sanity@4.10.3_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.14_@sanity+s_1530995b6ef1e06dc3fd05b18f3d2070/node_modules/sanity/bin/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/sanity@4.10.3_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.14_@sanity+s_1530995b6ef1e06dc3fd05b18f3d2070/node_modules/sanity/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/sanity@4.10.3_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.14_@sanity+s_1530995b6ef1e06dc3fd05b18f3d2070/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/sanity@4.10.3_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.14_@sanity+s_1530995b6ef1e06dc3fd05b18f3d2070/node_modules/sanity/bin/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/sanity@4.10.3_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.14_@sanity+s_1530995b6ef1e06dc3fd05b18f3d2070/node_modules/sanity/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/sanity@4.10.3_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.14_@sanity+s_1530995b6ef1e06dc3fd05b18f3d2070/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sanity" "$@"
else
  exec node  "$basedir/../../bin/sanity" "$@"
fi
