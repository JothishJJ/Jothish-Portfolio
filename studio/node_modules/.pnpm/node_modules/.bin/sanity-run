#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+runtime-cli@10.11.1_@types+node@24.8.1_debug@4.4.3_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+runtime-cli@10.11.1_@types+node@24.8.1_debug@4.4.3_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/runtime-cli/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+runtime-cli@10.11.1_@types+node@24.8.1_debug@4.4.3_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+runtime-cli@10.11.1_@types+node@24.8.1_debug@4.4.3_typescript@5.9.3_yaml@2.8.1/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+runtime-cli@10.11.1_@types+node@24.8.1_debug@4.4.3_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+runtime-cli@10.11.1_@types+node@24.8.1_debug@4.4.3_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/runtime-cli/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+runtime-cli@10.11.1_@types+node@24.8.1_debug@4.4.3_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+runtime-cli@10.11.1_@types+node@24.8.1_debug@4.4.3_typescript@5.9.3_yaml@2.8.1/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/runtime-cli/bin/run.js" "$@"
else
  exec node  "$basedir/../@sanity/runtime-cli/bin/run.js" "$@"
fi
