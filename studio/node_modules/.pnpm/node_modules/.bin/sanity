#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+cli@4.10.3_@types+node@24.8.1_react@19.2.0_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/cli/bin/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+cli@4.10.3_@types+node@24.8.1_react@19.2.0_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/cli/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+cli@4.10.3_@types+node@24.8.1_react@19.2.0_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+cli@4.10.3_@types+node@24.8.1_react@19.2.0_typescript@5.9.3_yaml@2.8.1/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+cli@4.10.3_@types+node@24.8.1_react@19.2.0_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/cli/bin/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+cli@4.10.3_@types+node@24.8.1_react@19.2.0_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/cli/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+cli@4.10.3_@types+node@24.8.1_react@19.2.0_typescript@5.9.3_yaml@2.8.1/node_modules/@sanity/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/@sanity+cli@4.10.3_@types+node@24.8.1_react@19.2.0_typescript@5.9.3_yaml@2.8.1/node_modules:/home/jothishjj/Documents/projects/JothishPortfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/cli/bin/sanity" "$@"
else
  exec node  "$basedir/../@sanity/cli/bin/sanity" "$@"
fi
